{
    "name": "uchup07/laravel-themes",
    "description": "Themes for laravel",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Uchup07\\LaravelThemes\\": "packages/uchup07/laravel-themes/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uchup07\\LaravelThemes\\ThemesServiceProvider"
            ],
            "aliases": {
                "Theme": "Uchup07\\LaravelThemes\\Facades\\Theme"
            }
        }
    },
    "authors": [
        {
            "name": "Yusuf Mulhajat",
            "email": "uchup_bell@yahoo.com"
        }
    ],
    "minimum-stability": "dev"
}
