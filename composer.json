{
    "name": "uchup07/laravel-themes",
    "description": "Themes for laravel",
    "keywords": [
        "uchup07",
        "laravel-themes",
        "themes"
    ],
    "type": "library",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "license": "MIT",
    "autoload": {
        "files": [
            "helpers/themes.php"
        ],
        "psr-4": {
            "Uchup07\\LaravelThemes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -d --update-snapshots",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "post-autoload-dump": [
            "vendor/bin/testbench package:discover --ansi"
        ]

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uchup07\\LaravelThemes\\LaravelThemesServiceProvider"
            ],
            "aliases": {
                "LaravelThemes": "Uchup07\\LaravelThemes\\Facades\\LaravelThemes"
            }
        }
    },
    "authors": [
        {
            "name": "Yusuf Mulhajat",
            "email": "uchup07@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev"
}
